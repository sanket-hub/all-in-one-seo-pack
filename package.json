{
	"name": "all-in-one-seo-pack",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"build:link-format-lite": "npm --prefix ./src/react/link-format run build-webpack-lite",
		"build:link-format-pro": "npm --prefix ./src/react/link-format run build-webpack-pro",
		"build:lite": "npm run env:lite && vite build && npm run build:link-format-lite && npm run pot",
		"build:pro": "npm run env:pro && vite build && npm run build:link-format-pro && npm run pot",
		"build": "npm run build:lite && npm run build:pro",
		"dev:lite": "npm run env:lite && vite",
		"dev:pro": "npm run env:pro && vite",
		"env:lite": "replace-in-files --regex='VITE\\_VERSION=(.)Pro(.)' --replacement='VITE_VERSION=$1Lite$2' ./build/.env",
		"env:pro": "replace-in-files --regex='VITE\\_VERSION=(.)Lite(.)' --replacement='VITE_VERSION=$1Pro$2' ./build/.env",
		"lint": "eslint --ext .js --ext .vue --ignore-path ../.eslintignore src",
		"pot": "./../_aioseo-scripts/pot.sh"
	},
	"engines": {
		"node": ">= 8.0.0"
	},
	"dependencies": {
		"@codemirror/lang-json": "6.0.1",
		"@codemirror/view": "6.12.0",
		"@emoji-mart/data": "1.1.2",
		"@popperjs/core": "2.11.7",
		"@varlet/ui": "2.11.1",
		"@wordpress/autop": "3.34.0",
		"@wordpress/block-editor": "12.2.0",
		"@wordpress/editor": "13.11.0",
		"@wordpress/i18n": "4.34.0",
		"animate-vanilla-js": "1.0.10",
		"apexcharts": "3.40.0",
		"clipboard": "2.0.11",
		"codemirror": "6.0.1",
		"element-plus": "2.3.5",
		"emoji-mart": "5.5.2",
		"libphonenumber-js": "^1.10.34",
		"lodash-es": "4.17.21",
		"lottie-web": "5.12.0",
		"luxon": "3.3.0",
		"maz-ui": "3.9.0",
		"popper.js": "1.15.0",
		"quill": "1.3.7",
		"superagent": "8.0.9",
		"tiny-emitter": "2.1.0",
		"tokenizer2": "2.0.1",
		"vue": "3.3.4",
		"vue-multiselect": "3.0.0-beta.1",
		"vue-router": "4.2.1",
		"vue-scrollto": "2.20.0",
		"vue3-apexcharts": "1.4.1",
		"vuedraggable": "4.1.0",
		"vuex": "4.1.0",
		"vuex-router-sync": "6.0.0-rc.1"
	},
	"devDependencies": {
		"@rollup/plugin-replace": "^5.0.2",
		"@vitejs/plugin-react": "^4.0.0",
		"@vitejs/plugin-vue": "^4.2.3",
		"@vue/compat": "3.3.4",
		"@vue/compiler-sfc": "3.3.4",
		"@vue/eslint-config-standard": "^8.0.1",
		"classnames": "^2.3.2",
		"dotenv": "^16.1.4",
		"eslint": "^8.42.0",
		"eslint-plugin-vue": "^9.14.0",
		"gettext-extractor": "^3.7.2",
		"htm": "3.1.1",
		"plural-forms": "^0.5.3",
		"pofile": "^1.1.4",
		"postcss": "^8.4.23",
